{
  "lexicon": 1,
  "id": "app.inkreaders.exercisePost",
  "defs": {
    "main": {
      "type": "record",
      "key": "tid",
      "record": {
        "type":"object",
        "required":["title","exercise"],
        "properties":{
          "title":{"type":"string","maxLength":200},
          "exercise":{"type":"ref","ref":"com.atproto.repo.strongRef"},
          "previewCount":{"type":"integer","minimum":1,"maximum":10},
          "createdAt":{"type":"string","format":"datetime"}
        }
      }
    }
  }
}
